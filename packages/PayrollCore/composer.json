{
    "name": "nexus/payroll-core",
    "description": "Payroll Core Domain Package - Pure business logic for payroll operations",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
