{
    "name": "nexus/accounting-operations",
    "description": "Nexus AccountingOperations Orchestrator - Cross-package workflow coordination for accounting processes",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "nexus/chart-of-account": "*@dev",
        "nexus/journal-entry": "*@dev",
        "nexus/period": "*@dev",
        "nexus/budget": "*@dev",
        "nexus/currency": "*@dev",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\AccountingOperations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\AccountingOperations\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
