{
    "name": "nexus/shared-kernel",
    "description": "Shared domain primitives, contracts, and value objects for Nexus packages",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\SharedKernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\SharedKernel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
