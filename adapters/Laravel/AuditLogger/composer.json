{
    "name": "nexus/laravel-auditlogger-adapter",
    "description": "Laravel adapter for AuditLogger package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nexus Development Team",
            "email": "dev@nexus-erp.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "nexus/auditlogger": "*@dev",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\Laravel\\AuditLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\Laravel\\AuditLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexus\\Laravel\\AuditLogger\\Providers\\AuditLoggerAdapterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
